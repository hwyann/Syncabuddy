<% @match.attendances.each do |attendance| %>
  <% if attendance.user.photo.attached? %>
    <%= cl_image_tag attendance.user.photo.key, height: 300, width: 400, crop: :fill, class: 'avatar', title: "#{attendance.user.nickname}" %>
  <% else %>
    <%= image_tag("avatar.jpg", class: 'avatar', title: "#{attendance.user.nickname}") %>
  <% end %>
<% end %>